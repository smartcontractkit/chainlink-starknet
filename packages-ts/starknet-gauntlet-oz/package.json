{
  "name": "@chainlink/starknet-gauntlet-oz",
  "version": "0.0.1",
  "description": "Starknet Gauntlet Open Zeppelin Contracts",
  "keywords": [
    "typescript",
    "cli"
  ],
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "scripts": {
    "gauntlet": "ts-node ./src/index.ts",
    "lint": "tsc",
    "test": "SKIP_PROMPTS=true jest --runInBand --passWithNoTests",
    "test:coverage": "yarn test --collectCoverage",
    "test:ci": "yarn test --ci",
    "format": "yarn prettier --write ./src",
    "format:check": "yarn prettier --check ./src",
    "clean": "rm -rf ./dist/ ./bin/",
    "build": "yarn clean && tsc -b",
    "bundle": "yarn build && pkg ."
  },
  "dependencies": {
    "@chainlink-dev/starkgate-open-zeppelin": "^0.0.4",
    "@chainlink/gauntlet-core": "0.3.1",
    "@chainlink/starknet-gauntlet": "*"
  }
}
